<script type="module" src="https://unpkg.com/@splinetool/viewer@1.9.23/build/spline-viewer.js"></script>
<spline-viewer class="hidden md:block" hint loading-anim-type="spinner-big-light" url="https://prod.spline.design/hxR9jgkzk3S1YxD0/scene.splinecode" loading="eager"></spline-viewer>
<script type="module">
    // Función para intentar manipular el componente después de que esté listo
    function attemptToStyleLogo() {
      const splineViewer = document.querySelector('spline-viewer');
  
      if (splineViewer) {
        // Verificar que el shadow DOM está disponible
        const shadowRoot = splineViewer.shadowRoot;
        
        if (shadowRoot) {
          const logo = shadowRoot.querySelector('#logo');
          if (logo) {
            logo.style.visibility = 'hidden';
            // Otros estilos
          } else {
            console.log('Elemento #logo no encontrado en el shadow DOM');
          }
        } else {
          console.log('shadowRoot no disponible');
        }
      } else {
        console.log('Componente spline-viewer no encontrado');
      }
    }
  
    // Intentar aplicar los estilos después de un breve retraso para asegurar que el componente esté cargado
    setTimeout(attemptToStyleLogo, 500); // Retraso de 1 segundo
  </script>



